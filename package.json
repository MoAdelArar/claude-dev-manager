{
  "name": "claude-dev-manager",
  "version": "1.0.2",
  "description": "Multi-agent development management system powered by Claude Code",
  "main": "dist/cli.js",
  "types": "dist/types.d.ts",
  "bin": {
    "cdm": "dist/cli.js",
    "cdm-mcp-server": "dist/mcp-server.js"
  },
  "files": [
    "dist",
    "templates",
    "commands",
    "scripts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "test": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e --testTimeout=120000",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "mcp": "node dist/mcp-server.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "pretest": "npm run typecheck",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "keywords": [
    "claude",
    "claude-code",
    "ai",
    "development",
    "project-management",
    "multi-agent",
    "software-engineering",
    "devops",
    "aws",
    "gcp",
    "azure"
  ],
  "author": "MoAdelArar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MoAdelArar/claude-dev-manager.git"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "enquirer": "^2.4.1",
    "ora": "^5.4.1",
    "uuid": "^10.0.0",
    "winston": "^3.17.0",
    "yaml": "^2.6.1",
    "zod": "^3.25.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
